{
  "name": "brick-engine-grpc",
  "version": "0.0.0",
  "description": "grpc server xboot engine",
  "main": "index.js",
  "types": "typings/index.d.ts",
  "scripts": {
    "docs": "jsdoc -c conf.json",
    "build": "tsc -d",
    "build:w": "tsc -d -w",
    "lint": "eslint --fix .",
    "check": "npm run build && npm run lint",
    "test": "npm run check && jest --coverage --env=node --runInBand",
    "watch": "jest --watch"
  },
  "files": [
    "typings",
    "package.json",
    "plugins",
    "plugin.js",
    "xboot.config.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kiba-zhao/brick-engine-grpc.git"
  },
  "keywords": [
    "grpc",
    "server",
    "engine",
    "brick-engine"
  ],
  "bugs": {
    "url": "https://github.com/kiba-zhao/brick-engine-grpc/issues"
  },
  "homepage": "https://github.com/kiba-zhao/brick-engine-grpc#readme",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.167",
    "@types/node": "^14.14.20",
    "eslint": "^7.17.0",
    "eslint-config-egg": "^9.0.0",
    "eslint-plugin-jest": "^24.1.3",
    "husky": "^4.3.7",
    "jest": "^26.6.3",
    "jsdoc": "^3.6.6",
    "typescript": "^4.1.3",
    "xboot": "^0.3.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "collectCoverageFrom": [
      "plugins/**/*.js"
    ]
  },
  "author": "kiba.x.zhao",
  "license": "MIT",
  "dependencies": {
    "brick-engine": "^0.1.4"
  }
}
